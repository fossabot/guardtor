  "keywords": [
    "security",
    "ddos",
    "validations",
    "bots",
    "bad bots",
    "scrapper",
    "tor",
    "anonymous",
    "spam",
    "protect",
    "php",
    "browser",
    "sanitize",
    "clean",
    "protect",
    "attack",
    "tor browser",
    "guard"
],
  "license": "MIT",
  "authors": [
    {
      "name": "Manomite",
      "email": "opensource@manomite.net"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable" : true,
  "require": {
    "php": ">=5.6",
    "matomo/device-detector": "^3.12",
    "beberlei/assert": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "GuardTor\\": [
        "src/"
      ]
    }
  }
}
